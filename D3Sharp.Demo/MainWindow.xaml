    <Grid Background="#EEEEF2">
        <!--<force:MutiForceView HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Background="Azure"/>-->
        <Frame x:Name="frame">
            <Frame.Content>
                <force:ForceDemoPage/>
            </Frame.Content>
        </Frame>
    </Grid>
</Window>
